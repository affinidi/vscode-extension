{
  "name": "affinidi",
  "publisher": "Affinidi",
  "displayName": "Affinidi",
  "description": "Fast-track your Web 2.0 to Web 3.0 journey with Affinidi",
  "author": "affinidi.com",
  "license": "Apache-2.0",
  "icon": "media/affinidi-icon-large.png",
  "preview": true,
  "repository": "https://gitlab.com/affinidi/foundational/genesis/vscode-extension",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "Affinidi",
    "snippet",
    "web3",
    "did",
    "credential"
  ],
  "activationEvents": [
    "onAuthenticationRequest:AffinidiOTP",
    "onCommand:affinidi.codegen.getIssuanceOffers",
    "onCommand:affinidi.codegen.sendVcOfferToEmail",
    "onCommand:affinidi.codegen.signVcWithCloudWallet",
    "onCommand:affinidi.copyJsonLdURL",
    "onCommand:affinidi.copyJsonURL",
    "onCommand:affinidi.createProject",
    "onCommand:affinidi.docs.availableSnippets",
    "onCommand:affinidi.login",
    "onCommand:affinidi.logout",
    "onCommand:affinidi.me",
    "onCommand:affinidi.openMarkDown",
    "onCommand:affinidi.signUp",
    "onCommand:affinidiDevTools.issueCredential",
    "onCommand:affinidiExplorer.createIssuance",
    "onCommand:affinidiExplorer.createSchema",
    "onCommand:affinidiExplorer.refresh",
    "onCommand:affinidiExplorer.showJsonLdContext",
    "onCommand:affinidiExplorer.showJsonSchema",
    "onCommand:affinidiExplorer.viewProperties",
    "onCommand:affinidiExplorer.initiateIssuanceCsvFlow",
    "onStartupFinished",
    "onView:affinidiCodeGeneration",
    "onView:affinidiDevTools",
    "onView:affinidiExplorer"
  ],
  "main": "./out/main",
  "contributes": {
    "configuration": {
      "title": "Affinidi",
      "properties": {
        "affinidi.telemetry.enabled": {
          "type": [
            "boolean",
            "null"
          ],
          "default": null,
          "markdownDescription": "%contributes.configuration.markdownDescription%",
          "scope": "window",
          "tags": [
            "telemetry"
          ]
        }
      }
    },
    "commands": [
      {
        "command": "affinidi.signUp",
        "title": "%affinidi.signUp%",
        "category": "Affinidi"
      },
      {
        "command": "affinidi.login",
        "title": "%affinidi.login%",
        "category": "Affinidi"
      },
      {
        "command": "affinidi.logout",
        "title": "%affinidi.signOut%",
        "category": "Affinidi"
      },
      {
        "command": "affinidi.createProject",
        "title": "%affinidi.createProject%",
        "category": "Affinidi"
      },
      {
        "command": "affinidi.me",
        "title": "%affinidi.me%",
        "category": "Affinidi",
        "icon": "$(account)"
      },
      {
        "command": "affinidi.codegen.sendVcOfferToEmail",
        "title": "%affinidi.codegen.sendVcOfferToEmail%",
        "category": "%commands.category.generators%"
      },
      {
        "command": "affinidi.codegen.getIssuanceOffers",
        "title": "%affinidi.codegen.getIssuanceOffers%",
        "category": "%commands.category.generators%"
      },
      {
        "command": "affinidi.codegen.signVcWithCloudWallet",
        "title": "%affinidi.codegen.signVcWithCloudWallet%",
        "category": "%commands.category.generators%"
      },
      {
        "command": "affinidiExplorer.refresh",
        "title": "%affinidiExplorer.refresh%",
        "category": "%commands.category.explorer%",
        "icon": "$(refresh)"
      },
      {
        "command": "affinidi.openMarkDown",
        "title": "%affinidi.openMarkDown%",
        "category": "%commands.category.explorer%",
        "icon": "$(info)"
      },
      {
        "command": "affinidi.copyJsonURL",
        "title": "%affinidi.copyJsonURL%",
        "category": "%commands.category.explorer%"
      },
      {
        "command": "affinidi.copyJsonLdURL",
        "title": "%affinidi.copyJsonLdURL%",
        "category": "%commands.category.explorer%"
      },
      {
        "command": "affinidiExplorer.viewProperties",
        "title": "%affinidiExplorer.viewProperties%",
        "category": "%commands.category.explorer%"
      },
      {
        "command": "affinidiExplorer.showJsonSchema",
        "title": "%affinidiExplorer.showJsonSchema%",
        "category": "%commands.category.explorer%"
      },
      {
        "command": "affinidiExplorer.showJsonLdContext",
        "title": "%affinidiExplorer.showJsonLdContext%",
        "category": "%commands.category.explorer%"
      },
      {
        "command": "affinidiExplorer.createSchema",
        "title": "%affinidiExplorer.createSchema%",
        "category": "%commands.category.explorer%"
      },
      {
        "command": "affinidiExplorer.createIssuance",
        "title": "%affinidiExplorer.createIssuance%",
        "category": "%commands.category.explorer%"
      },
      {
        "command": "affinidiDevTools.issueCredential",
        "title": "%affinidiDevTools.issueCredential%",
        "category": "%commands.category.devTool%"
      },
      {
        "command": "affinidi.docs.availableSnippets",
        "title": "%affinidi.docs.availableSnippets%",
        "category": "Affinidi"
      },
      {
        "command": "affinidi.codegen.app",
        "title": "%affinidi.codegen.app%",
        "category": "%commands.category.generators%"
      },
      {
        "command": "affinidiExplorer.initiateIssuanceCsvFlow",
        "title": "%affinidiExplorer.initiateIssuanceCsvFlow%",
        "category": "%commands.category.explorer%"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "affinidiExplorer.refresh",
          "when": "never"
        },
        {
          "command": "affinidi.openMarkDown",
          "when": "never"
        },
        {
          "command": "affinidi.copyJsonURL",
          "when": "never"
        },
        {
          "command": "affinidi.copyJsonLdURL",
          "when": "never"
        },
        {
          "command": "affinidiExplorer.initiateIssuanceCsvFlow",
          "when": "never"
        },
        {
          "command": "affinidiExplorer.viewProperties",
          "when": "never"
        },
        {
          "command": "affinidiExplorer.showJsonSchema",
          "when": "never"
        },
        {
          "command": "affinidiExplorer.showJsonLdContext",
          "when": "never"
        },
        {
          "command": "affinidiExplorer.createSchema",
          "when": "never"
        },
        {
          "command": "affinidiExplorer.createIssuance",
          "when": "never"
        },
        {
          "command": "affinidiDevTools.issueCredential",
          "when": "never"
        }
      ],
      "view/title": [
        {
          "command": "affinidiExplorer.refresh",
          "when": "view == affinidiExplorer",
          "group": "navigation"
        },
        {
          "command": "affinidi.me",
          "when": "view == affinidiExplorer",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "affinidiExplorer.refresh",
          "when": "view == affinidiExplorer && viewItem == rootSchemas || view == affinidiExplorer && viewItem == rootIssuance",
          "group": "inline"
        },
        {
          "command": "affinidi.openMarkDown",
          "when": "view == affinidiExplorer && viewItem == rootIssuance || view == affinidiExplorer && viewItem == rootSchemas",
          "group": "inline"
        },
        {
          "command": "affinidi.copyJsonURL",
          "when": "view == affinidiExplorer && viewItem == schema",
          "group": "6_copypath"
        },
        {
          "command": "affinidi.copyJsonLdURL",
          "when": "view == affinidiExplorer && viewItem == schema",
          "group": "6_copypath"
        },
        {
          "command": "affinidiExplorer.createSchema",
          "when": "view == affinidiExplorer && viewItem == rootSchemas",
          "group": "z_commands"
        },
        {
          "command": "affinidiExplorer.createIssuance",
          "when": "view == affinidiExplorer && viewItem == schema",
          "group": "z_commands"
        },
        {
          "command": "affinidiExplorer.initiateIssuanceCsvFlow",
          "when": "view == affinidiExplorer && viewItem == schema || view == affinidiExplorer && viewItem == issuance",
          "group": "1"
        },
        {
          "command": "affinidiExplorer.viewProperties",
          "when": "view == affinidiExplorer && viewItem == project || view == affinidiExplorer && viewItem == issuance || view == affinidiExplorer && viewItem == schema",
          "group": "z_commands"
        },
        {
          "command": "affinidiExplorer.showJsonSchema",
          "when": "view == affinidiExplorer && viewItem == schema",
          "group": "z_commands"
        },
        {
          "command": "affinidiExplorer.showJsonLdContext",
          "when": "view == affinidiExplorer && viewItem == schema",
          "group": "z_commands"
        },
        {
          "command": "affinidi.codegen.getIssuanceOffers",
          "when": "view == affinidiExplorer && viewItem == issuance",
          "group": "1"
        },
        {
          "command": "affinidi.codegen.sendVcOfferToEmail",
          "when": "view == affinidiExplorer && viewItem == schema",
          "group": "1"
        }
      ]
    },
    "views": {
      "affinidi": [
        {
          "id": "affinidiExplorer",
          "name": "%views.affinidi.affinidiExplorer%"
        },
        {
          "id": "affinidiCodeGeneration",
          "name": "%views.affinidi.affinidiCodeGeneration%"
        },
        {
          "id": "affinidiDevTools",
          "name": "%views.affinidi.affinidiDevTools%"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "affinidi",
          "title": "Affinidi",
          "icon": "media/affinidi-icon.png"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "affinidiDevTools",
        "contents": "%viewsWelcome.affinidiDevTools%"
      }
    ],
    "walkthroughs": [
      {
        "id": "affinidi-walkthrough",
        "title": "%walkthroughs.title%",
        "featuredFor": [],
        "description": "%walkthroughs.description%",
        "steps": [
          {
            "id": "createSchema",
            "title": "%walkthroughs.steps.0.title%",
            "description": "%walkthroughs.steps.0.description%",
            "media": {
              "image": "media/affinidi-logo.png",
              "altText": "Affinidi"
            },
            "completionEvents": [
              "onCommand:getting-started.createschema"
            ]
          },
          {
            "id": "createRule",
            "title": "%walkthroughs.steps.1.title%",
            "description": "walkthroughs.steps.1.description",
            "media": {
              "image": "media/affinidi-logo.png",
              "altText": "Affinidi"
            },
            "completionEvents": [
              "onSettingChanged:getting-started.sampleSetting"
            ]
          }
        ]
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      }
    ],
    "authentication": [
      {
        "id": "AffinidiOTP",
        "label": "%authentication.title%"
      }
    ]
  },
  "l10n": "./l10n",
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/main.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run clean && npm run compile",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "node ./out/test/runTest.js",
    "clean": "rimraf ./out",
    "dev": "nodemon -x 'npm run compile'",
    "generate-translation": "npx @vscode/l10n-dev export -o ./l10n ./src"
  },
  "devDependencies": {
    "@affinidi/eslint-config": "^2.0.0-beta.0",
    "@affinidi/prettier-config": "^2.0.0-beta.0",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/deep-equal-in-any-order": "^1.0.1",
    "@types/glob": "^8.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.2",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@types/source-map-support": "^0.5.6",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/l10n-dev": "^0.0.18",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "deep-equal-in-any-order": "^2.0.0",
    "esbuild": "^0.15.13",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "sinon": "^14.0.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@affinidi/client-iam": "^0.3.1",
    "@affinidi/client-issuance": "^0.4.0",
    "@affinidi/client-schema-manager": "^0.3.1",
    "@affinidi/client-user-management": "^0.3.2",
    "@vscode/l10n": "^0.0.10",
    "@vscode/test-electron": "^2.1.5",
    "@vscode/webview-ui-toolkit": "^1.1.0",
    "conf": "^10.2.0",
    "date-fns": "^2.29.3",
    "execa": "^5.1.1",
    "form-data": "^4.0.0",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^8.5.1",
    "lru-cache": "^7.14.1",
    "nanoid": "^3.3.4",
    "node-fetch": "^2.6.7",
    "path": "^0.12.7"
  }
}
